{
  "name": "MCMirror",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^7.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "cweagans/composer-patches": "*",
    "php-pm/httpkernel-adapter": "dev-master",
    "php-pm/php-pm": "*",
    "ramsey/uuid": "*",
    "sensio/framework-extra-bundle": "*",
    "symfony/asset": "*",
    "symfony/console": "*",
    "symfony/finder": "*",
    "symfony/flex": "*",
    "symfony/framework-bundle": "*",
    "symfony/profiler-pack": "*",
    "symfony/translation": "*",
    "symfony/twig-bundle": "*",
    "symfony/web-server-bundle": "*",
    "symfony/webpack-encore-bundle": "*",
    "symfony/yaml": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "symfony/maker-bundle": "*",
    "symfony/dotenv": "*",
    "friendsofphp/php-cs-fixer": "*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false
    },
    "enable-patching": true,
    "patches": {
      "php-pm/httpkernel-adapter": {
        "Fix Webpack Encore Bundle EntrypointLookup reset": "https://patch-diff.githubusercontent.com/raw/php-pm/php-pm-httpkernel/pull/153.patch"
      }
    }
  }
}
